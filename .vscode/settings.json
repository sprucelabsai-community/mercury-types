{
    "debug.node.autoAttach": "on",
    "git.ignoreLimitWarning": true,
    "javascript.validate.enable": false,
    "editor.formatOnSave": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "build": true,
        "**/node_modules/**": true,
        "**/.next/**": true
    },
    "search.exclude": {
        "**/build/**": true,
        "**/node_modules/**": true,
        "**/.next/**": true
    },
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "[javascriptreact]": {
        "editor.formatOnSave": false
    },
    "[typescript]": {
        "editor.formatOnSave": false
    },
    "[typescriptreact]": {
        "editor.formatOnSave": false
    },
    "[handlebars]": {
        "editor.formatOnSave": false
    },
    "eslint.workingDirectories": ["./"],
    "eslint.options": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.preferences.importModuleSpecifier": "relative",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/build/**",
        "**/vscode-extension/**",
        "**/.git/objects/**",
        ".vscode",
        ".spruce"
    ],
    "cSpell.words": [
		"Booter",
		"Cruding",
		"Orgs",
		"arkit",
		"autogenerated",
		"portscanner",
		"socketio",
		"spreedsheet",
		"tacobell",
		"upsert",
		"upserted"
	],
    "files.exclude": {}
}
